{
  "name": "react-redux-ssr",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "webpack --config ./webpack/webpack.dev.config.js && node ./dist/server.js",
    "dev": "webpack --config ./webpack/webpack.dev.config.js --progress --watch & node ./dist/server.js",
    "build": "NODE_ENV=production webpack -p --config ./webpack/webpack.prod.config.js",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:js": "eslint ./src",
    "flow": "flow",
    "test:ava": "NODE_ENV=test ava",
    "test:jest": "NODE_ENV=test jest",
    "test:cover": "nyc --reporter=lcov --reporter=text yarn test:ava"
  },
  "dependencies": {
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "ava": "^0.25.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.65.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.2.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "sass-loader": "^6.0.7",
    "serialize-javascript": "^1.4.0",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/setup"
    ],
    "files": [
      "src/**/*.test.js"
    ]
  }
}
